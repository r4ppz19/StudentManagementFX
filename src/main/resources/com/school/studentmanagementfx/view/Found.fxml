<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" maxWidth="180.0" prefHeight="90.0" prefWidth="180.0" spacing="10.0" style="-fx-border-color: #1E1E2E; -fx-border-width: 1; -fx-background-radius: 5px; -fx-border-radius: 5px; -fx-border-width: 0.5;" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.school.studentmanagementfx.controller.FoundStudentController">
   <children>
      <Label prefHeight="23.0" prefWidth="37.0" text="Found" textFill="#1e1e2e">
         <font>
            <Font name="Segoe UI Black" size="12.0" />
         </font>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </Label>
      <Button mnemonicParsing="false" onAction="#onViewAction" prefHeight="25.0" prefWidth="75.0" style="-fx-background-color: #1E1E2E; -fx-background-radius: 5px; -fx-border-radius: 5px;" text="View" textFill="WHITE">
         <font>
            <Font name="Segoe UI Black" size="10.0" />
         </font>
      </Button>
   </children>
</VBox>
