<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="232.0" prefWidth="280.0" spacing="20.0"
    stylesheets="@../style/Shared.css" xmlns="http://javafx.com/javafx/23.0.1"
    xmlns:fx="http://javafx.com/fxml/1"
    fx:controller="com.school.studentmanagementfx.controller.MainMenuController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <Button focusTraversable="false" onAction="#addStudentBtn" prefHeight="34.0" prefWidth="200.0"
        text="Add student" />
    <Button focusTraversable="false" onAction="#displayStudentBtn" prefHeight="34.0"
        prefWidth="200.0" text="Display student" />
    <Button focusTraversable="false" onAction="#searchStudentBtn" prefHeight="34.0"
        prefWidth="200.0" text="Search student" />
</VBox>